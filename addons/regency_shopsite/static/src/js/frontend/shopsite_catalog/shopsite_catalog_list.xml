<?xml version="1.0" encoding="UTF-8"?>
<templates>

    <t t-name="shopsite_catalog_list" owl="1">
        <div>
            <t t-foreach="Object.values(productList)" t-as="product" t-key="product_index">
                <div>
                    <div>
                        <img t-att-src="product.main_image_url"/>
                    </div>
                    <div t-esc="product.name"/>
                    <br/>
                </div>

            </t>
        </div>
    </t>

</templates>